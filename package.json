{
  "name": "ohmstudio",
  "version": "1.0.0",
  "description": "portable virtual modular synth platform that aims to be fun, usable, collaborative, and extensible. UI designed for ohmbre hardware synthesizer or similar touchscreen device, yet it is fully cross-platform.",
  "bin": "run",
  "scripts": {
    "install": "npm install ./mathjs && (npm install ./alsa || npm install ./node-core-audio)"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ohmbre/ohmstudio.git"
  },
  "keywords": [
    "synthesizer",
    "audio",
    "video",
    "modular",
    "nodejs",
    "qml",
    "js"
  ],
  "author": "Matt Gattis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ohmbre/ohmstudio/issues"
  },
  "homepage": "https://github.com/ohmbre/ohmstudio#readme",
  "dependencies": {
    "alsa": "file:alsa",
    "mathjs": "file:mathjs",
    "node-core-audio": "file:node-core-audio",
    "ws": "^5.2.0"
  },
  "devDependencies": {},
  "optionalDependencies": {
    "bufferutil": "^3.0.5"
  }
}
